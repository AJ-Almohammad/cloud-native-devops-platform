{
    "name": "multi-everything/content-api",
    "description": "Content API microservice for Multi-Everything DevOps platform",
    "type": "project",
    "require": {
        "php": "^8.2",
        "slim/slim": "4.x",
        "slim/psr7": "^1.6",
        "monolog/monolog": "^2.8",
        "illuminate/database": "^10.0",
        "illuminate/events": "^10.0",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}